syntax enable

set tabstop=4
set softtabstop=4
set expandtab

set number
set showcmd
filetype indent on
set wildmenu
set lazyredraw 
set showmatch

set incsearch
set hlsearch

nnoremap <silent> <C-L> :nohlsearch<CR><C-L>

set foldenable
set foldlevelstart=10
set foldnestmax=10
nnoremap <space> za
set foldmethod=indent


